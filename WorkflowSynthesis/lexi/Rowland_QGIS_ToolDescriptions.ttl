    wf:input1 [ a ccd:FieldRaster, ccd:NominalA ];
    wf:input2 [ a ccd:Lattice, ccd:VectorA ];
    wf:output [ a ccd:Lattice, ccd:VectorA, ccd:NominalA ].
    
